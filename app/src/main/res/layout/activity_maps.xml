<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tent"
    tools:context=".MapsActivity">

    <TextView
        android:layout_width="196dp"
        android:layout_height="90dp"
        android:layout_marginTop="100dp"
        android:text="@string/title"
        android:textAllCaps="false"
        android:textColor="#FBFAF9"
        android:textColorHighlight="#F4EDEC"
        android:textSize="36sp"
        android:textStyle="bold|italic"
        android:typeface="serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="417dp"
        android:layout_height="729dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <!--1 hamahiga-->

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:background="#800A0A0A"
                android:paddingLeft="24dp"
                android:paddingRight="20dp"
                android:layout_marginTop="350dp"
                android:text="@string/hamahiga"
                android:textAlignment="center"
                android:textColor="#F5F3F2"
                android:textSize="30sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:background="#800A0A0A"
                android:paddingLeft="20dp"
                android:text="@string/hamahiga.text"
                android:textColor="#F7F6F6"
                android:textSize="18sp"
                android:textStyle="bold" />

            <fragment
                android:id="@+id/fragment1"
                android:name="com.google.android.youtube.player.YouTubePlayerFragment"
                android:layout_width="match_parent"
                android:layout_height="208dp"
                android:layout_marginLeft="20dp"

                android:layout_marginRight="20dp" />

            <fragment
                android:id="@+id/map1"
                android:name="com.google.android.gms.maps.SupportMapFragment"
