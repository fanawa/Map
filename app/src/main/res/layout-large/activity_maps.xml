<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tent"
    tools:context=".MapsActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="418dp"
        android:layout_height="157dp"
        android:src="@drawable/araha1" />

    <TextView
        android:layout_width="188dp"
        android:layout_height="77dp"
        android:text="@string/title"
        android:textAllCaps="false"
        android:textColor="#FBFAF9"
        android:textColorHighlight="#F4EDEC"
        android:textSize="36sp"
        android:textStyle="bold|italic"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="187dp"
        android:layout_height="293dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="180dp"
        android:orientation="vertical"
        android:weightSum="0.3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="24dp"
            android:paddingRight="20dp"
            android:text="@string/hamahiga"
            android:textAlignment="center"
            android:textColor="#F5F3F2"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:text="@string/hamahiga.text"
            android:textColor="#F7F6F6"
            android:textSize="18sp"
            android:textStyle="bold" />

        <fragment
            android:id="@+id/fragment1"
            android:name="com.google.android.youtube.player.YouTubePlayerFragment"
            android:layout_width="match_parent"
            android:layout_height="208dp"

            />

        <fragment
            android:id="@+id/map1"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="248dp"
            tools:context=".MapsActivity" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="187dp"
        android:layout_height="298dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="180dp"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout1"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/yabuchi"
            android:textAlignment="center"
            android:textColor="#F5F3F2"
            android:textSize="30sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:text="@string/yabuchi.text"
            android:textColor="#F7F6F6"
            android:textSize="18sp"
            android:textStyle="bold" />

        <fragment
            android:id="@+id/fragment2"
            android:name="com.google.android.youtube.player.YouTubePlayerFragment"
            android:layout_width="match_parent"
            android:layout_height="208dp"

            />

        <fragment
            android:id="@+id/map2"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="248dp"
            tools:context=".MapsActivity" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="175dp"
        android:layout_height="298dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="180dp"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout2"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/uku"
            android:textAlignment="center"
            android:textColor="#F5F3F2"
            android:textSize="30sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:text="@string/uku.text"
            android:textColor="#F7F6F6"
            android:textSize="18sp"
            android:textStyle="bold" />

        <fragment
            android:id="@+id/fragment3"
            android:name="com.google.android.youtube.player.YouTubePlayerFragment"
            android:layout_width="match_parent"
            android:layout_height="208dp"

            />

        <fragment
            android:id="@+id/map3"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="248dp"
            tools:context=".MapsActivity" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="195dp"
        android:layout_height="290dp"
        android:layout_marginTop="100dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout1">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/akuna"
            android:textAlignment="center"
            android:textColor="#F5F3F2"
            android:textSize="30sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:text="@string/akuna.text"
            android:textColor="#F7F6F6"
            android:textSize="18sp"
            android:textStyle="bold" />

        <fragment
            android:id="@+id/fragment4"
            android:name="com.google.android.youtube.player.YouTubePlayerFragment"
            android:layout_width="match_parent"
            android:layout_height="208dp"

            />

        <fragment
            android:id="@+id/map4"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="248dp"
            tools:context=".MapsActivity" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="169dp"
        android:layout_height="304dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="100dp"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout3"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/w.b"
            android:textAlignment="center"
            android:textColor="#F5F3F2"
            android:textSize="30sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#800A0A0A"
            android:paddingLeft="20dp"
            android:text="@string/w.b.text"
            android:textColor="#F7F6F6"
            android:textSize="18sp"
            android:textStyle="bold" />

        <fragment
            android:id="@+id/fragment5"
            android:name="com.google.android.youtube.player.YouTubePlayerFragment"
            android:layout_width="match_parent"
            android:layout_height="208dp"

            />

        <fragment
            android:id="@+id/map5"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="248dp"
            tools:context=".MapsActivity" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>